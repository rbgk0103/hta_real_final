<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="menu">
	<select id = 'select' parameterType="String" resultType="bean.MenuVo"> <!-- 리스트 검색(select) -->
		select * from menu where menu_type = #{find}
	</select>
</mapper>